{
  "manifest_version": 2,

  "name": "YNAB MBNA UPLOADER",
  "description": "This extension uploads MBNA transactions to YNAB",
  "version": "0.1",

  "content_scripts": [
	  {
	  	"matches": ["https://service.mbna.ca/waw/mbna/accountSnapshot.action"],
	  	"css": ["mymain.css"],
      "js": ["bank.js", "transactions.js", "myjs.js"]
	  }
	],

  "page_action": {
  	"default_icon": "img.png"
  }
}